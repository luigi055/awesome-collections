{
  "name": "awesome-collections",
  "version": "0.0.0",
  "description": "LinkedList, Deque, Stack, Queue, heaps, and other useful data structures missing in ES. keeping similar API syntax and behavior as Map, Set and Array",
  "main": "index.js",
  "scripts": {
    "build": "lerna run build --stream",
    "lint": "eslint . --ext .ts  --cache --cache-strategy content",
    "test": "lerna run test",
    "prepare": "husky install && pnpm --silent build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1",
    "lerna": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
